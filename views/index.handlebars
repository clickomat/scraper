<div>
	<h1 style="background: linear-gradient(to right , rgba(0, 247, 255, 0.904), rgb(150, 78, 245));"class="card z-depth-3 center-align">Read the Latest articles Page</h1>
	<div class="row">
		<div class="col s3"></div>
		<h5   style="background: linear-gradient(to right , rgba(0, 247, 255, 0.904), rgb(150, 78, 245));" class="card z-depth-3 center-align col s6">Click 'Scrape' above.</h5>
	</div>
</div>

<div class="row">
	<div class="col s2"></div>
	<div class="col s8 left-align">
		{{#each article}}
		<div style="background: linear-gradient(to right , rgba(0, 247, 255, 0.904), rgb(150, 78, 245));"  class="card-panel">
			<h5>{{this.title}}</h5>

			<a href="/readArticle/{{this._id}}" class="waves-effect waves-light btn blue lighten-1" id="btnSignUp">Read Article</a>
		</div>
		{{/each}}
	</div>
</div>